<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
                               http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="sylius.import_export.csv_exporter.class">Sylius\Bundle\ImportExportBundle\Exporter\CsvExporter</parameter>
    </parameters>

    <services>
        <service id="sylius.import_export.csv_exporter" class="%sylius.import_export.csv_exporter.class%">
            <argument type="service" id="doctrine.orm.entity_manager" />
        </service>
    </services>

</container>
